div>\n\n            <div>\n              <Link to=\"/table\">\n                <ButtonStyle>Submit</ButtonStyle>\n              </Link>\n            </div>\n          </Form>\n        </PickerDiv>\n      </Div>\n    </>\n  )\n}\n\nexport default PickerSection\n","/Users/tai/Dropbox/code/DeriOne-frontend/src/components/globalStyles/globalStyles.js",["79"],"import { createGlobalStyle, css } from \"styled-components\"\nimport BG from \"../../resources/11.jpg\"\n\nexport default createGlobalStyle`\n    html {\n        height: 100%;\n    }\n\n    body{\n        display:flex;\n        flex-direction: column;\n        height: 100%;\n        line-height: 1.6;\n        background: url(${BG}) no-repeat center center/cover;\n        \n    }\n\n    * {\n        box-sizing: border-box;\n        padding: 0;\n        margin: 0;\n        font-family: 'Didact Gothic', sans-serif;\n    }\n\n\n`\n","/Users/tai/Dropbox/code/DeriOne-frontend/src/components/header/header.styles.js",[],"/Users/tai/Dropbox/code/DeriOne-frontend/src/components/optionsTable/optionsTable.style.js",[],"/Users/tai/Dropbox/code/DeriOne-frontend/src/components/pickerSection/pickerSection.style.js",[],"/Users/tai/Dropbox/code/DeriOne-frontend/src/components/rangeDropdown/index.js",[],"/Users/tai/Dropbox/code/DeriOne-frontend/src/components/rangeDropdown/rangeDropdown.style.js",[],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":9,"column":25,"nodeType":"86","messageId":"87","endLine":9,"endColumn":30},{"ruleId":"84","severity":1,"message":"88","line":9,"column":32,"nodeType":"86","messageId":"87","endLine":9,"endColumn":38},{"ruleId":"84","severity":1,"message":"89","line":16,"column":7,"nodeType":"86","messageId":"87","endLine":16,"endColumn":10},{"ruleId":"84","severity":1,"message":"90","line":29,"column":10,"nodeType":"86","messageId":"87","endLine":29,"endColumn":36},{"ruleId":"84","severity":1,"message":"91","line":30,"column":10,"nodeType"